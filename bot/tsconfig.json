{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": false,
      "skipLibCheck": true,
      "outDir": "dist",
      "resolveJsonModule": true
    },
    "include": ["./**/*.ts"],
    "ts-node": {
      "esm": false,
      "compilerOptions": {
        "module": "commonjs"
      }
    }
  }
  